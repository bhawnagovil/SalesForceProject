package Com.Day1;
import java.util.Scanner;
public class FactorialNumber {

	
	public int findFactorial(int n) {
		
	
		int i, fact=1;
		for(i=n; i<=1; i--) {
			fact= fact*i;
			
		}
			
		return fact;
				
	}
	
	public static void main(String[] args) {
		
    Scanner sc= new Scanner(System.in);
    
	}

}
